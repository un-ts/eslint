{
  "name": "eslint-plugin-utils",
  "version": "0.4.1",
  "type": "module",
  "description": "Utilities for ESLint plugins and custom rules",
  "repository": "git+https://github.com/un-ts/eslint.git",
  "homepage": "https://github.com/un-ts/eslint/blob/main/packages/utils",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "funding": "https://opencollective.com/eslint-plugins",
  "license": "MIT",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "main": "./lib/index.cjs",
  "types": "./index.d.cts",
  "module": "./index.d.cts",
  "exports": {
    "import": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
