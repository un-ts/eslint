{
  "name": "eslint-plugin-htm",
  "version": "0.3.0",
  "description": "An incredible ESLint plugin for HTML based on HTMLHint.",
  "repository": "git+https://github.com/rx-ts/eslint.git",
  "homepage": "https://github.com/rx-ts/eslint/blob/main/packages/htm",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=v14.17.1"
  },
  "main": "lib",
  "module": "lib/esm",
  "es2015": "lib/es2015",
  "fesm5": "lib/esm",
  "types": "lib",
  "files": [
    "lib",
    "!*.tsbuildinfo"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-plugin-htm",
    "eslint-plugin-html",
    "markuplint",
    "htmllint",
    "htmlhint",
    "html-lint",
    "html-hint",
    "linthtml"
  ],
  "peerDependencies": {
    "eslint": ">=5.0.0"
  },
  "dependencies": {
    "eslint-plugin-utils": "^0.1.0",
    "htmlhint": "^0.15.1",
    "tslib": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "./lib/es*.js",
    "./lib/index.js"
  ]
}
