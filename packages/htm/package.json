{
  "name": "eslint-plugin-htm",
  "version": "1.0.0",
  "type": "module",
  "description": "An incredible ESLint plugin for HTML based on HTMLHint",
  "repository": "git+https://github.com/un-ts/eslint.git",
  "homepage": "https://github.com/un-ts/eslint/blob/main/packages/htm",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "funding": "https://opencollective.com/eslint-plugins",
  "license": "MIT",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "main": "./lib/index.cjs",
  "types": "./index.d.cts",
  "module": "./index.d.cts",
  "exports": {
    "import": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "require": {
      "types": "./index.d.cts",
      "default": "./lib/index.cjs"
    }
  },
  "files": [
    "index.d.cts",
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-plugin-htm",
    "eslint-plugin-html",
    "markuplint",
    "htmllint",
    "htmlhint",
    "html-lint",
    "html-hint",
    "linthtml"
  ],
  "peerDependencies": {
    "@typescript-eslint/utils": ">=7.0.0",
    "eslint": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/utils": {
      "optional": true
    }
  },
  "dependencies": {
    "@pkgr/core": "^0.2.4",
    "eslint-plugin-utils": "^0.4.0",
    "htmlhint": "^1.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "./lib/index.cjs",
    "./lib/index.js"
  ]
}
