{
  "name": "eslint-plugin-text",
  "version": "1.0.0",
  "type": "module",
  "description": "An incredible ESLint plugin for retext or textlint",
  "repository": "git+https://github.com/un-ts/eslint.git",
  "homepage": "https://github.com/un-ts/eslint/blob/main/packages/text",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "funding": "https://opencollective.com/eslint-plugins",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./lib/index.cjs",
  "types": "./index.d.cts",
  "module": "./index.d.cts",
  "exports": {
    "import": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "require": {
      "types": "./index.d.cts",
      "default": "./lib/index.cjs"
    }
  },
  "files": [
    "index.d.cts",
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-plugin-text",
    "retext",
    "textlint",
    "spell",
    "spellcheck",
    "spelling",
    "spelling-check"
  ],
  "peerDependencies": {
    "@typescript-eslint/utils": ">=7.0.0",
    "eslint": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/utils": {
      "optional": true
    }
  },
  "dependencies": {
    "@textlint/config-loader": "^14.6.0",
    "eslint-plugin-utils": "^0.4.0",
    "lilconfig": "^3.1.3",
    "retext-stringify": "^4.0.0",
    "synckit": "^0.11.4",
    "textlint": "^14.6.0",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "vfile-message": "^4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "./lib/index.cjs",
    "./lib/index.js"
  ]
}
